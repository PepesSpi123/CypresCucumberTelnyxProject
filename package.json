{
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.0.0",
    "@cypress/browserify-preprocessor": "latest",
    "cypress": "latest"
  },
  "scripts": {
    "test:chrome": "cypress run --browser chrome",
